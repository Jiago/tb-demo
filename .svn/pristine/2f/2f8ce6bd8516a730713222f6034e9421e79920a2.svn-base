<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/stay_evaluate_backbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/stay_evaluate"
            android:textColor="@drawable/reg_bt_on"
            android:textSize="@dimen/mytaobaotextSize"
            android:textStyle="bold" 
            android:background="#FFFFFF"
            android:drawableLeft="@drawable/mini_back"/>

        
    </LinearLayout>

    <ListView
        android:id="@+id/stay_eveluate_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_weight="1.0"
        android:divider="@drawable/reg_bt_on" >
    </ListView>

</RelativeLayout>